{
    "builds": [{
        "src": "django_lang_translator/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django_lang_translator/wsgi.py"
        }
    ]
}